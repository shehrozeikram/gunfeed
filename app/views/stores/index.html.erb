<!DOCTYPE html>
<html lang="en">
<%= render partial: 'partials/title' %>
<body class="body-style">
<%= render partial: 'partials/header' %>
<section>
  <div class="container">
    <div class="add-section add-banner-section">
    </div>
  </div>
</section>
<!-- ----------------- store section start here ----------------------->

<section>
  <div class="container">
    <div class="row">
      <div class="col-lg-12 pb-3">
        <div class="row row-flex-sm row-flex-md row-flex-lg">
          <div class="col-xs-12 col-sm-9">
            <div class="card card--listing-header p-3">
              <div class="card__title">
                <h1 class="listing-title listing-title--block"> Stores <div class="subtitle subtitle--listing"> Showcasing total of <span class="pager-total-items" id="pager-total-items"><%=@stores.all.count %></span> stores </div>
                </h1>
              </div>
            </div>
          </div>
          <div class="col-xs-12 col-sm-3 text-center">
            <div class="card card--vertical-center-content">
              <div class="card__content">
                <a class="btn btn-primary btn-block text-white" href="#" target="_blank">View valid coupons</a>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12">
          <span class="popular-mark">Search Stores</span>
        </div>
        <div class="col-xs-12 col-md-3">
          <div class="form-group">
            <input type="text" placeholder="Enter the name of the store" id="search-stores" class="form-control js-search-store">
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-12">
          <span class="popular-mark">Most Popular Stores</span>
        </div>
      </div>
    </div>
    <div class="row row-cols-5 card-devider">
      <% @stores.each do |store| %>
      <div class="col my-3 sub-card">

        <div class="card h-100">
          <div class="tile tile--store">
            <div class="tile__title">
              <p><%= store.title %></p>
            </div>
          </div>
          <div class="tile__actions-container card-footer">
            <a href="#" class="card-links icon-fa-link"><i class="fa fa-eye"></i><span>Deals</span></a>
            <a href="#" class="card-links icon-fa-link"><i class="fa fa-scissors"></i><span>Coupons</span></a>
          </div>
        </div>

      </div>
      <% end %>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <div class="card card--status is-dealer">
          <div class="card__content">
            <fieldset class="fieldset-no-label d-flex">
              <div class="" id="edit-expires-wrapper">
                <div class="js-field-toggle field-toggle field-toggle--switch js-linked-input field-toggle--light field-toggle--mini field-toggle--with-title field-toggle--title-after" data-linked-input="#edit-expires-date-wrapper">
                  <div class="field-toggle__container">
                    <label class="btn-switch">
                      <input type="checkbox">
                      <span class="btn-slider btn-round"></span>
                    </label>
                  </div>
                  <span class="field-toggle__title js-field-toggle-title noselect" role="button">  Only with deals  </span>
                </div>
              </div>
              <div class="" id="edit-expired-wrapper">
                <div class="js-field-toggle field-toggle field-toggle--switch field-toggle--light field-toggle--mini field-toggle--with-title field-toggle--title-after">
                  <div class="field-toggle__container">
                    <label class="btn-switch">
                      <input type="checkbox">
                      <span class="btn-slider btn-round"></span>
                    </label>
                  </div>
                  <span class="field-toggle__title js-field-toggle-title noselect" role="button">  Only with coupons  </span>
                </div>
              </div>
            </fieldset>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
