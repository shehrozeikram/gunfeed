                         value="form-mHD9hWW6AfpichtQut1i9KdoOcGkEQL5bwTNa4VFE-w">
                  <input type="hidden" name="form_token" id="edit-form-token"
                         value="a123ec0f2f5c93657fb78d48ba4e0d6d">
                  <input type="hidden" name="form_id" id="edit-comment-form" value="comment_form">
                  <fieldset class=" collapsible collapsed">
                    <legend class="collapse-processed">
                      <a href="#">Images</a>
                    </legend>
                    <div class="fieldset-wrapper">
                      <div class="js-dropzone dropzone dz-clickable">
